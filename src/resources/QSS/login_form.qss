QToolButton#toolButtonExit
{
    border-image:url(:/Images/Exit.png); 
}

QToolButton#toolButtonExit:hover
{
    border-image:url(:/Images/Exit.png)3 3 3 3;
    background-color: rgba(255,255,255,0.2);
    border-radius: 4px;
}

QToolButton#toolButtonExit:pressed
{
    border-image:url(:/Images/Exit.png)1 4 2 1; 
    background-color: rgba(0,0,0,0.2);
    border-radius: 4px;
}